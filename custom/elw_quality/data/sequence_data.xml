<odoo>
    <data>
        <record id="seq_elw_quality_point" model="ir.sequence">
            <field name="name">Elw Quality Point Sequence</field>
            <field name="code">elw.quality.point.sequence</field>
            <field name="prefix">QCP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/> <!-- False : common for all companies-->
        </record>

        <record id="seq_elw_quality_check" model="ir.sequence">
            <field name="name">Elw Quality Check Sequence</field>
            <field name="code">elw.quality.check.sequence</field>
            <field name="prefix">QC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/> <!-- False : common for all companies-->
        </record>

    </data>
</odoo>