<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate = '0'>
        <record id="ir_cron_elw_auto_email" model="ir.cron">
            <field name="name">Scheduled Calibration: Send Notifications</field>
            <field name="model_id" ref="elw_maintenance.model_elw_maintenance_calibration" />
            <field name="state">code</field>
            <field name="code">model.action_send_email_on_scheduled_date()</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">2</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <!-- <field name="nextcall" eval="(DateTime.now().replace(hour=0, minute=30) +
            timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />  -->
        </record>
    </data>
